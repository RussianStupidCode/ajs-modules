{
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "no-console": "off",
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ],
        "max-classes-per-file": [
            "error",
            { "ignoreExpressions": true, "max": 2 }
        ]
   }
}